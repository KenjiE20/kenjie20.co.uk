{
    "ci": {
        "collect": {
            "numberOfRuns": 3,
            "staticDistDir": "./public",
            "settings": {
                "preset": "desktop",
                "skipAudits": ["is-on-https"]
            }
        },
        "assert": {
            "preset": "lighthouse:recommended",
            "assertions": {
                "link-in-text-block": "off",
                "color-contrast": "off"
            }
        }
    }
}